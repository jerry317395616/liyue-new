.page {
  position: relative;
  padding: 20rpx 20rpx 0 20rpx;
}
.page .top-box {
  width: 100%;
  box-sizing: border-box;
}
.page .top-box .top-box1 {
  position: relative;
  width: 98%;
  height: 300rpx;
  background-color: #E99136;
  border-radius: 50rpx;
  margin-right: 10rpx;
}
.page .top-box .top-box1 .img1 {
  position: absolute;
  bottom: -45rpx;
  left: 0;
  width: 100rpx;
}
.page .top-box .top-box1 .top-box1-text {
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  font-size: 38rpx;
  font-weight: bold;
}
.page .top-box .top-box1 .img2 {
  position: absolute;
  bottom: 30rpx;
  right: 50rpx;
  width: 110rpx;
}
.page .top-box .top-box2 {
  position: relative;
  width: 98%;
  height: 600rpx;
  background-color: #E1D3B6;
  border-radius: 50rpx;
  margin-top: 15rpx;
  margin-left: 10rpx;
}
.page .top-box .top-box2 .img1 {
  position: absolute;
  top: -45rpx;
  right: 0;
  width: 100rpx;
}
.page .top-box .top-box2 .top-box2-text {
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  width: 100%;
}
.page .top-box .top-box2 .top-box2-text .top-box2-child1 {
  font-size: 38rpx;
  font-weight: bold;
}
.page .top-box .top-box2 .top-box2-text .top-box2-child2 {
  color: #7B776F;
  margin-left: 30rpx;
  margin-top: 30rpx;
  font-weight: bold;
}
.page .top-box .top-box2 .top-box2-text .top-box2-child3 {
  color: #7B776F;
  margin-left: 30rpx;
  font-weight: bold;
  margin-top: 30rpx;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox {
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding-right: 80rpx;
  margin-top: 120rpx;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox .leftSchedule {
  display: flex;
  justify-content: start;
  align-items: center;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox .leftSchedule .circle {
  width: 20rpx;
  height: 20rpx;
  background-color: #525252;
  border-radius: 50%;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox .leftSchedule .text {
  margin: auto 0 auto 24rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #525252;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox1 {
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding-right: 80rpx;
  margin-top: 40rpx;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox1 .leftSchedule {
  display: flex;
  justify-content: start;
  align-items: center;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox1 .leftSchedule .circle {
  width: 20rpx;
  height: 20rpx;
  background-color: #525252;
  border-radius: 50%;
}
.page .top-box .top-box2 .top-box2-text .scheduleBox1 .leftSchedule .text {
  margin: auto 0 auto 24rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #525252;
}
.page .footer-box {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 200rpx;
  width: 100%;
  background-color: #000;
  padding: 20rpx;
}
.page .footer-box .buy-box {
  display: flex;
  justify-content: center;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 100rpx;
  background-color: #E1D3B6;
  color: #6E4A15;
  text-align: center;
  border-radius: 50rpx;
  margin: 10rpx auto auto auto;
  line-height: 100rpx;
  font-size: 28rpx;
  font-weight: bold;
}
.page .footer-box .buy-box .buy-text {
  font-size: 40rpx;
}
.page .footer-box .buy-box .buy-text1 {
  margin-left: 30rpx;
}
.page .footer-box .buy-box .buy-text1 .buy-text3 {
  font-size: 55rpx;
}
 .u-checkbox__label {
  margin-right: 0 !important;
}
